#!/bin/bash

xrandr | grep -q "DP1-8 connected"
if [ $? -eq 0 ]
then
  echo "dock"
  xrandrSetup --dock
  exit 0;
fi

xrandr | grep -q "DP2 connected"
if [ $? -eq 0 ]
then
  echo "dock"
  xrandrSetup --dock
  exit 0;
fi

xrandr | grep "HDMI1 connected"
if [ $? -eq 0 ]
then
  echo "hdmi"
  xrandrSetup --hdmi
  exit 0
fi

echo "nothing"
xrandrSetup 

